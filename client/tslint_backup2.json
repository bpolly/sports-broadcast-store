{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [false],
        "semicolon": [false],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any",
                "named-imports-order": "case-insensitive"
            }
        ],
        "member-access": [
            false
        ],
        "jsx-curly-spacing": [true, "always"],
        "arrow-parens": [false],
        "@typescript-eslint/no-explicit-any": "off"
    },
    "rulesDirectory": []
}
